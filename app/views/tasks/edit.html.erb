<h1>Edit new tasks</h1>


<%= form_for @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :details %>
  <%= f.text_field :details %>

  <%= f.label :completed %>
  <%= f.check_box :completed %>

  <%= f.submit %>

<% end %>



<p>
<%= link_to 'Back to list', tasks_path %>
</p>
